[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
[user]
	name = Marco Munizaga
	email = marco.munizaga@ufl.edu
[diff]
	tool = vimdiff
[merge]
	tool = vim
[mergetool "vim"]
	cmd = vim \"+Gdiff\" $MERGED
